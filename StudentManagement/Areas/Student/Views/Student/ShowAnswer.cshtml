@model StudentManagement.Areas.Student.Data.ShowAnswerModel

@{
    ViewBag.Title = "Answer";
}
@if (Model.Test != null)
{
    @Html.Partial("_TestDetail", Model.Test)
}
<!-- DataTales Example -->
<div class="card shadow mb-4">
    <div class="card-header ">
        <h3 class="m-0 text-primary font-weight-bold">Answer</h3>
    </div>
    <div class="card-body">
        @if (Model.Answer != null)
        {
            @Html.Partial("_AnswerDetail", Model.Answer)
        }
        else
        {
            @: &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; No answer found...
        }

    </div>
</div>
